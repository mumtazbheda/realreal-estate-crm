{
  "buildCommand": "cd apps/miniapp && pnpm run build",
  "devCommand": "cd apps/miniapp && pnpm run dev",
  "installCommand": "pnpm install",
  "framework": null,
  "outputDirectory": "apps/miniapp/.next",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/apps/api/:path*"
    }
  ],
  "crons": [
    {
      "path": "/api/health",
      "schedule": "0 0 * * 0"
    }
  ]
}
