{
  "version": 2,
  "builds": [
    {
      "src": "apps/miniapp/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/apps/api/$1"
    }
  ],
  "functions": {
    "apps/api/**/*.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "crons": []
}
